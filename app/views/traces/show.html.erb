<!-- Content Header (Page header) -->
<section class="content-header">
  <h1>
    Trace
  </h1>
  <ol class="breadcrumb">
  </ol>
</section>

<% if false %>
<section class="content">
  <div class="row">
    <div class="col-lg-12 connectedSortable">
      <div class="box">
        <div class="box-header">
          <h3 class="box-title">Transaction Sample <%= @trace.duration.to_f %></h3>
        </div>
        <div class="box-body">
          <table class="table">
            <%= render @trace.root_sample, :depth => 0, :version => 2, :previous_margin => 0, :start_time => @trace.root_sample.timestamp, :end_time => @trace.root_sample.timestamp + @trace.root_sample.duration %>
          </table>
        </div>
      </div>
    </div>
  </div>
</section>
<% elsif true %>
<section class="content">
  <div class="row">
    <div class="col-lg-12 connectedSortable">
      <div class="box">
        <div class="box-header">
          <h3 class="box-title">Transaction Sample <%= @trace.duration.to_f %></h3>
        </div>
        <div class="box-body">
          <table class="table">
            <%= render @root_with_children, :depth => 0, :version => 1 %>
          </table>
        </div>
      </div>
    </div>
  </div>
</section>
<% end %>
